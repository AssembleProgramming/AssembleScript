                            Return in switch
========================================================================== $

def switchTest(c){
    multiverse(c){
        madness 0:
            vision(0);
            return "Case 0";
            vision(0);
        madness 1:
            vision(1);
            return "Case 1";
            endGame;
            vision(1);
        madness 2:
            vision(2);
            return "Case 2";
            vision(2);
        madness 3:
            vision(3);
            return "Case 3";
            vision(3);
        default:
            vision(4);
            return "Default";
            vision(4);
    }
}

def stringSwitchTest(c){
    multiverse(c){
        madness "one":
            vision(1);
            return "String Case 1";
            vision(1);
        madness "two":
            vision(2);
            return "String Case 2";
            vision(2);
        madness "three":
            vision(3);
            return "String Case 3";
            vision(3);
        madness "four":
            vision(4);
            return "String Case 4";
            vision(4);
        default:
            vision(100);
            return "String Case Default";
            vision(100);
            endGame;
    }
}

vision(stringSwitchTest("1"));